<header>
  <div class="header-inner">
    <h1>
      <a href="/" class="text-primary">
        ã‚“.
      </a>
    </h1>

    <div id="navbar-mobile-header">
      <div class="vseparator"></div>
      <div id="mobile-nav-sect-label">UwU</div>
      <div class="flex-space"></div>
      <button class="navbar-button icon-only blank" id="navbar-button">{{{icons.bars}}}</button>
    </div>

    <div id="navbar-desktop-header">
      <div class="flex-space"></div>
      <nav>
        <ul class="nav-inner-center">
          {{#each nav.links}}
            {{#unless this.external}}
              <li class="scrollspy">
                <a href="/{{this.url}}" class="nav-link">
                  {{{this.icon}}}
                  {{{this.name}}}
                </a>
              </li>
            {{/unless}}
          {{/each}}
        </ul>
      </nav>

      <div class="vseparator"></div>

      <nav>
        <ul class="nav-inner-right">
          {{#each nav.links}}
            {{#if this.external}}
              <li>
                <a href="{{this.url}}" class="nav-link"
                  target="_blank"
                  rel="me nofollow noopener noreferrer"
                  referrerpolicy="no-referrer"
                  aria-label="{{this.url}}"
                >
                  {{{this.icon}}}
                  {{{this.name}}}
                </a>
              </li>
            {{/if}}
          {{/each}}
        </ul>
      </nav>
    </div>
  </div>

  <nav id="navbar-mobile" class="collapsed">
    <div class="nav-button-container">
      <button class="nav-item navbar-button nav-link icon-only blank">{{{icons.xmark}}}</button>
    </div>
    <ul>
      {{#each nav.links}}
        {{#unless this.external}}
          <li>
            <a href="{{this.url}}" class="nav-link">
              {{{this.icon}}}
              {{{this.name}}}
            </a>
          </li>
        {{/unless}}
      {{/each}}
    </ul>
    <ul class="nav-vertical">
      {{#each nav.links}}
        {{#if this.external}}
          <li>
            <a href="{{this.url}}" class="nav-link"
              target="_blank"
              rel="nofollow noopener noreferrer"
              referrerpolicy="no-referrer"
            >
              {{{this.icon}}}
              {{{this.name}}}
            </a>
          </li>
        {{/if}}
      {{/each}}
    </ul>
  </nav>
</header>
<div id="collapse-trigger-area" class="hidden"></div>